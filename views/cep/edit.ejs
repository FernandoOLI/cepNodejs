<%- include ../layouts/header.ejs %>

<% if (messages.error) { %>
<p style="color:red"><%- messages.error %></p>
<% } %>

<% if (messages.success) { %>
<p style="color:green"><%- messages.success %></p>
<% } %>

<form action="/cep/edit/<%= cep %>" method="post" name="form1">
	<div>
		<td>Cep edit <%= cep  %></td>
	</div>
	<table width="25%" border="0">
		<div>
			<tr>
				<td>Nome</td>
				<td><input type="text" name="nome" id="nome" value="<%= nome  %>" /></td>
			</tr>
			<tr>
				<td>Endereco</td>
				<td><input type="text" name="endereco" id="endereco" value="<%= endereco %>" /></td>
			</tr>
			<tr>
				<td>Bairro</td>
				<td><input type="text" name="bairro" id="bairro" value="<%= bairro %>" /></td>
			</tr>
			<tr>
				<td>Estado</td>
				<td><input type="text" name="estado" id="estado" value="<%= estado %>" /></td>
			</tr>
			<tr>
				<td>Cidade</td>
				<td><input type="text" name="cidade" id="cidade" value="<%= cidade %>" /></td>
			</tr>
			<tr>
				<td></td>
				<td><input type="submit" name="Submit" value="Editar" /></td>
			</tr>

		</div>
	</table>
	<input type="hidden" name="_method" value="PUT" />
</form>
<%- include ../layouts/footer.ejs %>